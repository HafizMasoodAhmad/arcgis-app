@using System.Text.Json
@if (TempData["ScenarioRawJson"] is string raw)
{
    <script>
        const rawJson = @Html.Raw(JsonSerializer.Serialize(raw));
        sessionStorage.setItem('pdot:scenario:rawJson', rawJson);
        sessionStorage.setItem('pdot:scenario:pendingImport', '1');
    </script>
}


